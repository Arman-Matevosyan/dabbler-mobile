{
  "name": "mixandfit",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "optimize": "npx expo-optimize",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:android:prod": "eas build --platform android --profile production",
    "build:ios:prod": "eas build --platform ios --profile production",
    "update:preview": "eas update --branch preview --message \"Manual preview update\"",
    "update:production": "eas update --branch production --message \"Manual production update\"",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "prepare-release": "npm run lint && npm run typecheck && npm run optimize"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5.1.1",
    "@hookform/resolvers": "^4.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/react-query": "^5.67.2",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "expo": "~52.0.42",
    "expo-auth-session": "~6.0.3",
    "expo-barcode-scanner": "^13.0.1",
    "expo-blur": "~14.0.3",
    "expo-camera": "~16.0.18",
    "expo-font": "~13.0.4",
    "expo-image-picker": "^16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-location": "~18.0.10",
    "expo-router": "~4.0.20",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "i18next": "^24.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-native": "0.76.8",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "1.18.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "^0.19.13",
    "react-native-webview": "13.12.5",
    "zod": "^3.24.2",
    "zustand": "^5.0.3",
    "expo-calendar": "~14.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.11.5",
    "@types/react": "~18.3.12",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.5",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
